<%= render  partial: '/layouts/nav' %>
<div class="container">
  <h1 class="center">Welcome
    <%= @user.email %>
  </h1>

  <div id="markets">
    <div class="row">
      <div class="col s7 pull-s5">
        <div class="card">
          <div class="card-content">
            <div class="card-title">
              Markets
            </div>
            <table>
            <% @markets.each do |market| %>
              <tr><%= link_to  market.name, market_path(market) %></tr>
            <% end %>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
  <%= render partial: '/layouts/current_temp' %>
  <%= render partial: '/layouts/forecast' %>



</div>